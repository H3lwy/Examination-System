  "components": {
    "schemas": {
      "AnswerDto": {
        "type": "object",
        "properties": {
          "questionId": {
            "type": "integer",
            "format": "int32"
          },
          "selectedChoiceId": {
            "type": "integer",
            "format": "int32"
          }
        },
        "additionalProperties": false
      },
      "ChoiceDto": {
        "type": "object",
        "properties": {
          "choiceText": {
            "type": "string",
            "nullable": true
          },
          "isCorrect": {
            "type": "boolean"
          }
        },
        "additionalProperties": false
      },
      "ChoicesDto": {
        "type": "object",
        "properties": {
          "questionId": {
            "type": "integer",
            "format": "int32"
          },
          "choices": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/ChoiceDto"
            },
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "ExamDto": {
        "type": "object",
        "properties": {
          "examName": {
            "type": "string",
            "nullable": true
          },
          "subjectId": {
            "type": "integer",
            "format": "int32"
          },
          "timeLimit": {
            "type": "string",
            "format": "date-span"
          },
          "passScore": {
            "type": "integer",
            "format": "int32"
          }
        },
        "additionalProperties": false
      },
      "ExamSubmissionDto": {
        "type": "object",
        "properties": {
          "studentId": {
            "type": "string",
            "nullable": true
          },
          "examId": {
            "type": "integer",
            "format": "int32"
          },
          "subjectId": {
            "type": "integer",
            "format": "int32"
          },
          "answers": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/AnswerDto"
            },
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "QuestionDto": {
        "type": "object",
        "properties": {
          "questionText": {
            "type": "string",
            "nullable": true
          },
          "examId": {
            "type": "integer",
            "format": "int32"
          }
        },
        "additionalProperties": false
      },
      "QuestionWithChoicesDto": {
        "type": "object",
        "properties": {
          "questionText": {
            "type": "string",
            "nullable": true
          },
          "choices": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/ChoiceDto"
            },
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "SubjectDto": {
        "required": [
          "subjectDescription",
          "subjectName"
        ],
        "type": "object",
        "properties": {
          "subjectName": {
            "minLength": 1,
            "type": "string"
          },
          "subjectDescription": {
            "minLength": 1,
            "type": "string"
          }
        },
        "additionalProperties": false
      },
      "UpdateUserStatusDto": {
        "required": [
          "isActive"
        ],
        "type": "object",
        "properties": {
          "isActive": {
            "type": "boolean"
          }
        },
        "additionalProperties": false
      }
    }
