        "tags": [
          "Account"
        ],
        "requestBody": {
          "content": {
            "multipart/form-data": {
              "schema": {
                "required": [
                  "ConfirmPassword",
                  "Email",
                  "FirstName",
                  "LastName",
                  "Password",
                  "UserName"
                ],
                "type": "object",
                "properties": {
                  "FirstName": {
                    "type": "string"
                  },
                  "LastName": {
                    "type": "string"
                  },
                  "UserName": {
                    "type": "string"
                  },
                  "Password": {
                    "type": "string",
                    "format": "password"
                  },
                  "ConfirmPassword": {
                    "type": "string",
                    "format": "password"
                  },
                  "Email": {
                    "type": "string",
                    "format": "email"
                  }
                }
              },
              "encoding": {
                "FirstName": {
                  "style": "form"
                },
                "LastName": {
                  "style": "form"
                },
                "UserName": {
                  "style": "form"
                },
                "Password": {
                  "style": "form"
                },
                "ConfirmPassword": {
                  "style": "form"
                },
                "Email": {
                  "style": "form"
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
